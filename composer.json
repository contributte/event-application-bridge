{
  "name": "contributte/event-application-bridge",
  "description": "Bridge between event dispatcher and nette application",
  "keywords": ["nette", "event", "dispatcher", "bridge", "application"],
  "type": "library",
  "license": ["MIT"],
  "homepage": "https://github.com/contributte/event-application-bridge",
  "authors": [
    {
      "name": "Milan Felix Sulc",
      "homepage": "https://f3l1x.io"
    }
  ],
  "require": {
    "php": ">= 5.6",
    "nette/di": "^2.4.8",
    "nette/application": "^2.4.6."
  },
  "require-dev": {
    "ninjify/qa": "^0.4.0",
    "ninjify/nunjuck": "^0.1.4",
    "contributte/event-dispatcher": "^0.3.0"
  },
  "autoload": {
    "psr-4": {
      "Contributte\\Events\\Bridges\\Application\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Fixtures\\": "tests/fixtures"
    }
  }
}
